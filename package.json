{
  "name": "nativescript-images-generator-hook",
  "version": "3.0.1",
  "description": "Nativescript hook that generates platforms images based on a single high resolution image.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Creatiwity/nativescript-images-generator-hook"
  },
  "bugs": {
    "url": "https://github.com/Creatiwity/nativescript-images-generator-hook/issues"
  },
  "keywords": [
    "nativescript",
    "hook",
    "images",
    "ios",
    "android",
    "resources",
    "resize"
  ],
  "author": "Julien Blatecky <julien.blatecky@creatiwity.net> (https://github.com/Julien1619)",
  "license": "MIT",
  "homepage": "https://github.com/Creatiwity/nativescript-images-generator-hook#readme",
  "nativescript": {
    "platforms": {
      "android": "7.0.1",
      "ios": "7.2.0"
    },
    "hooks": [
      {
        "type": "before-prepare",
        "script": "lib/before-prepare.js",
        "inject": true
      }
    ]
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "preuninstall": "node preuninstall.js"
  },
  "dependencies": {
    "sharp": "^0.25.2"
  }
}
